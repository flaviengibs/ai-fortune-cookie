{
  "manifest_version": 3,
  "name": "AI Fortune Cookie",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/cookie16.png",
      "48": "images/cookie48.png",
      "128": "images/cookie128.png"
    }
  },
  "permissions": ["storage", "https://api.openai.com/"],
  "icons": {
    "128": "fortunecookie.png"
  }
}
